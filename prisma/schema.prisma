generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Contrato {
  id String @id
  numero_contrato String
  valor_contrato String
  data_vigencia String
  fornecedor String
  cnpj String
  aditivo String
  cps Boolean
  ata_registro Boolean
  porcentagem_reajuste String?
  responsavel_contrato String
  numero_edital String
  numero_lote String
  hora_sem_orcamento String
  numero_processo String
  numero_ata String
  hora_fora_contrato String
  observacao String
  @@map("dbo.contrato")
}

model Item { 
  id String @id
  numero_item Int
  perfil String
  quantidade_hora String
  valor_hora String
  numero_contrato String
  @@map("dbo.item")
}

model Colaborador {
  id String @id
  perfil String
  colaborador String
  telefone String
  cpf_rg String
  email String
  aprovado Boolean
  numero_contrato String
  @@map("dbo.colaborador")
}

model Demanda {
  id String @id
  titulo_projeto String
  epic_projeto Int
  contrato_filho String
  fornecedor String
  cnpj String
  descricao String?
  responsavel String
  projeto Boolean
  sustentacao Boolean
  numero_contrato String
  observacao String?
  @@map("dbo.demanda")
}

model Estimativa {
  id String @id
  numero_item String
  titulo_requisito String
  json_perfil Json
  numero_contrato String
  valor_total Float
  horas Float
  observacao String?
  @@map("dbo.estimativa")
}
